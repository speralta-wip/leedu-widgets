{
  "name": "leedu-widgets",
  "version": "0.0.8",
  "description": "Leedu webcomponents widgets",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/leedu-widgets/leedu-widgets.esm.js",
  "exports": {
    ".": {
      "import": "./dist/leedu-widgets/leedu-widgets.esm.js",
      "require": "./dist/leedu-widgets/leedu-widgets.cjs.js"
    },
    "./leedu-form": {
      "import": "./dist/components/leedu-form.js",
      "types": "./dist/components/leedu-form.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@stencil/sass": "^3.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0"
  },
  "license": "MIT"
}
